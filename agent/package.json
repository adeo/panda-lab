{
  "name": "PandaLab",
  "version": "0.1.0",
  "main": "background.js",
  "private": true,
  "scripts": {
    "prebuild": "node scripts/protobuf.js && node scripts/genEnv.js",
    "serve": "npm run prebuild && vue-cli-service serve",
    "build": "npm run prebuild && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "npm run prebuild && npm run build:agent",
    "build:agent": "script-file",
    "electron:serve": "npm run prebuild && vue-cli-service electron:serve",
    "electron:deploy": "npm run electron:build && node scripts/uploadAgent.js",
    "postinstall": "node scripts/postinstall.js && electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@google-cloud/firestore": "2.2.7",
    "@types/chart.js": "^2.8.5",
    "adbkit": "^2.11.1",
    "axios": "^0.21.1",
    "bufferutil": "^4.0.1",
    "chart.js": "^2.8.0",
    "core-js": "^2.6.9",
    "electron-store": "^4.0.0",
    "express": "^4.17.1",
    "firebase": "^6.5.0",
    "firebase-admin": "^8.2.0",
    "firebaseui": "^4.1.0",
    "follow-redirects": "^1.9.0",
    "get-port": "^5.0.0",
    "grpc": "^1.23.3",
    "guid-typescript": "^1.0.9",
    "pandalab-commons": "file:../commons",
    "protobufjs": "^6.8.8",
    "request": "^2.88.0",
    "request-progress": "^3.0.0",
    "rmdir-recursive": "0.0.1",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.2",
    "stream": "0.0.2",
    "utf-8-validate": "^5.0.2",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2",
    "vue-class-component": "^7.0.2",
    "vue-material": "^1.0.0-beta-11",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.1.2",
    "vue-rx": "^6.2.0",
    "vue-rx-decorators": "0.0.8",
    "webpack-proto-loader": "^0.5.2",
    "winston": "^3.2.1",
    "ws": "^7.1.2",
    "ws-avc-player": "^2.0.2",
    "xhr2": "^0.2.0",
    "xmlhttprequest": "^1.8.0",
    "zip-folder": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/ws": "^6.0.3",
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-plugin-typescript": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "copy-webpack-plugin": "^5.0.4",
    "electron": "^5.0.0",
    "envfile": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "script-file": "^0.12.0",
    "typescript": "^3.5.3",
    "vue-cli-plugin-electron-builder": "^1.4.0",
    "vue-template-compiler": "^2.6.10"
  },
  "engines": {
    "node": "8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "rules": {
      "no-console": "off",
      "no-restricted-syntax": [
        "error",
        {
          "selector": "CallExpression[callee.object.name='console'][callee.property.name!=/^(log|warn|error|info|trace)$/]",
          "message": "Unexpected property on console object was called"
        }
      ]
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
